#!/usr/bin/env bash
#I keep all my local aliases and exports here. Usually filepath dependent aliases and sensitive data. This is just a sample

#Aliases
  #Runescape
  alias osrsins="install_osrs()" #run to update aswell
  alias osrs="java -Duser.home=$HOME -Dsun.java2d.uiScale=2 -Dsun.java2d.xrender=True -Dsun.java2d.opengl=true -Dsun.java2d.xrender=True -Djava.class.path=/usr/local/bin/jagexappletviewer.jar -Dcom.jagex.config=http://oldschool.runescape.com/jav_config.ws jagexappletviewer /usr/local/share/"
#Exports
  export DFileRepo="https://github.com/zack1119/Dotfiles.git"
  #Email
  export Email_User="TestUsr"
  export Email_Pass="TestPass"
  #SQL Test Server
  export SQL_Server="HostName"
  export SQL_User="TestUsr"
  export SQL_Pass="TestPass"
